Machine=`uname -m | sed "s/_/-/g"`
Platform=`uname | tr '[:upper:]' '[:lower:]'`
if [[ $Platform == "darwin" ]]; then
    Platform="osx"
fi
export CPLEX_ROOT="$HOME/Application/CPLEX_Studio1210"
export CPLEX_ARCH=${Machine}_${Platform}
export prefix=/usr/local
export PY=python3
export CC=clang++-10
